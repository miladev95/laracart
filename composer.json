{
    "name": "miladev/lara-cart",
    "description": "Laravel shopping cart",
    "version" : "1.3",
    "keywords": [
        "Cart",
        "Shopping cart",
        "Laravel shopping cart",
        "Laravel cart",
        "Laravel php cart"
    ],
    "homepage": "https://github.com/miladev95/lara-cart",
    "license": "MIT",
    "authors": [
        {
            "name": "Miladev",
            "email": "Miladev95@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Miladev\\Laracart\\" : "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Miladev\\Laracart\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miladev\\Laracart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Miladev\\Laracart\\Facades\\Cart"
            }
        }
    }
}
